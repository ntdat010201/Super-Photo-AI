# Weather API Configuration
# Copy this file to .env and fill in your actual values

# Weather API Base URL
WEATHER_API_BASE_URL=https://your-weather-api-domain.com

# Weather API Key from The Weather Company (IBM)
WEATHER_API_KEY=your_weather_api_key_here

# Default coordinates (Hanoi, Vietnam)
DEFAULT_LATITUDE=21.0285
DEFAULT_LONGITUDE=105.8542

# API Configuration
WEATHER_API_UNITS=m
WEATHER_API_LANGUAGE=vi-VN